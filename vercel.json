{
    "version": 2,
    "builds": [
        {
            "src": "/index.js",
            "use": "@now/node-server"
        },
        {
            "src": "/apiMethods/**",
            "use": "@now/static"
        },
        {
            "src": "/data/**",
            "use": "@now/static"
        },
        {
            "src": "/static/**",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": ".*",
            "methods": [
                "GET",
                "POST",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept",
                "Access-Control-Allow-Credentials": "true"
            },
            "dest": "index.js",
            "continue": true,
            "cleanUrls": true
        }
    ],
    "github": {
        "enabled": false
    }
}